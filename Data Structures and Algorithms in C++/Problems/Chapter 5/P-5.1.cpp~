 * 	
 * 	typedef string Elem;
 * 	class LinkedDeque {
 * 	public:
 * 		LinkedDeque();
 * 		int size() const;
 * 		bool empty() const;
 * 		const Elem& front() const throw(DequeEmpty);
 * 		const Elem& back() const throw(DequeEmpty);
 * 		void insertFront(const Elem& e);
 * 		void insertBack(const Elem& e);
 * 		void removeFront() throw(DequeEmpty);
 * 		void removeBack() throw(DequeEmpty);
 * 	private:
 * 		DLinkedList D;
 * 		int n;
 * 	};
